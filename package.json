{
  "name": "@risemaxi/sigil",
  "version": "1.2.8",
  "description": "Icons and Signs for the Rise Design System",
  "homepage": "https://risevest.com",
  "main": "build/index.js",
  "exports": {
    ".": {
      "import": "./build/index.js",
      "require": "./build/index.js",
      "types": "./build/index.d.ts"
    },
    "./flags": {
      "import": "./build/flags.js",
      "require": "./build/flags.js",
      "types": "./build/flags.d.ts"
    },
    "./types": {
      "import": "./build/types.js",
      "require": "./build/types.js",
      "types": "./build/types.d.ts"
    }
  },
  "types": "build/index.d.ts",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "format": "bunx biome check --write icon-pack *.ts",
    "prepare": "bun run build",
    "generate": "bun ./scripts/build-icons.js"
  },
  "keywords": [
    "sigil",
    "remix icons",
    "lucide icons",
    "Deji.zeal",
    "icon",
    "drawable",
    "icon library",
    "icon pack"
  ],
  "files": [
    "build"
  ],
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/risevest/sigil.git"
  },
  "bugs": "https://github.com/risevest/sigil/issues",
  "contributors": [
    "Rise Developers"
  ],
  "peerDependencies": {
    "react-native-svg": ">=12"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.2",
    "@types/react": "18",
    "camelcase": "^6.0.0",
    "cheerio": "^1.0.0",
    "glob": "^7.1.6",
    "react": "*",
    "react-native-svg": "^15.11.1",
    "uppercamelcase": "^3.0.0",
    "typescript": "~5.0.0"
  }
}
